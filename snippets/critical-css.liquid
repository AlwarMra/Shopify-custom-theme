/* PREDICTIVE SEARCH */
.search-modal__form {
  width: 100%;
}
.predictive-search {
display: none;
position: absolute;
top: calc(100% + 0.1rem);
width: calc(100% + 0.2rem);
left: 1rem;
box-shadow: 0 0 5px rgb(0 0 0 / 25%);
background-color: white;
z-index: 6;
border-radius: 4px;
top: 70px; 
max-width: 750px; 
}

.predictive-arrow {
  position: absolute;
  top: -14px;
  left: 30px;
  float: left;
  overflow: hidden;
  width: 21px;
  height: 14px;
}
  .predictive-arrow-outer, 
  .predictive-arrow-inner { 
  position: absolute;
  top: 1px;
  left: 0;
  display: inline-block;
  margin-left: -2px;
}
.predictive-arrow-outer {
border-right: 11px solid rgba(255,255,255,0);
  border-left: 11px solid rgba(255,255,255,0);
  border-bottom: 15px solid #ddd;
  
}
.predictive-arrow-inner {
top: 2px;
  left: 2px;
  display: inline-block;
  border-right: 9px solid rgba(255,255,255,0);
  border-left: 9px solid rgba(255,255,255,0);
  border-bottom: 13px solid #fff;
}
.arrow-color {
border-bottom: 13px solid var(--main-color);
}
#predictive-search-results-list,
#predictive-search-results-list-cols {
  display: block;
}
.predictive-search--search-template {
z-index: 2
}

@media screen and (max-width: 870px) {
  .search-container-menu {
    position: fixed;
    background: var(--contrast-color);
    padding: 1rem;
    top: 116px;
    left: 0;
    width: 100%;
